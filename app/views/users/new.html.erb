<h2> Welcome to our site</h2>
<p>Please sign up below</p>
<div class= "center">
<%= form_for @user do |f| %>
<p><%= f.label :username %></br>
<%= f.text_field :username %> 
</p>
<p>
    <%= f.label :password %></br>
    <%= f.text_field :password %>
</p>
<p>
    <%= f.submit 'signup' %>
</p>
<% end %>
</div>